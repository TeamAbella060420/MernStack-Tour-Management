ky\r\n        }\r\n    })\r\n}\r\n\r\nuseEffect(() => { // useEffect hook that calls stickyHeaderFunc to update sticky navbar\r\n  stickyHeaderFunc()\r\n  return () => window.removeEventListener('scroll', stickyHeaderFunc) // this cleanup function is to remove the event listener to prevent memory leak\r\n})\r\n\r\n  return (\r\n    <header className=\"header\" ref={headerRef}>\r\n      <Container>\r\n        <Row>\r\n          <div className=\"nav__wrapper d-flex align-items-center justify-content-between\">\r\n            {/* ============== logo =================*/}\r\n              <div className=\"logo\">\r\n                <img src={logo} alt=\"\" />\r\n              </div>\r\n            {/* ============== logo end =================*/}\r\n            {/* ============== menu start =================*/}\r\n\r\n          <div className=\"navigation\">\r\n            <ul className=\"menu d-flex align-items-center gap-5\">\r\n              {\r\n                nav__links.map((item, index) => (\r\n                  <li className='nav__item' key={index}>\r\n                      <a>\r\n                    <NavLink to={item.path} className={navClass => navClass.isActive ? 'active__link' : ''}>\r\n                      {item.display}\r\n                    </NavLink>\r\n                      </a>\r\n                  </li>\r\n                ))\r\n              }\r\n            </ul>\r\n          </div>\r\n\r\n          <div className=\"nav__right d-flex align-items-center gap-4\">\r\n            <div className=\"nav__btns d-flex align-items-center gap-4\">\r\n              <Button className='btn secondary__btn'>\r\n                <Link to='/login'>\r\n                  Login\r\n                </Link>\r\n              </Button>\r\n              <Button className='btn primary__btn'>\r\n                <Link to='/register'>\r\n                  Register\r\n                </Link>\r\n              </Button>\r\n            </div>\r\n            <span className=\"mobile__menu\">\r\n              <i class=\"ri-menu-line\"></i>\r\n            </span>\r\n          </div>\r\n          </div>\r\n        </Row>\r\n      </Container>\r\n    </header>\r\n  )\r\n}\r\n\r\nexport default Header","C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\components\\Footer\\Footer.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\pages\\Login.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\pages\\Home.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\pages\\Register.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\pages\\SearchResultList.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\pages\\TourDetails.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\pages\\Tours.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\utils\\avgRating.js",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\assets\\data\\tours.js",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\shared\\Subtitle.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\shared\\CommonSection.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\shared\\SearchBar.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\shared\\NewsLetter.jsx",[],[],"C:\\Web Development\\FullStack\\MernStack-Tour-Management\\tour-management\\frontend\\src\\shared\\TourCard.jsx",["141"],[],"import React from \"react\";\r\nimport { Card, CardBody } from \"reactstrap\";\r\nim